<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE LIMES SYSTEM "limes.dtd">
<LIMES>
   <PREFIX>
      <NAMESPACE>http://data.organic-edunet.eu/lom_ontology_organicEdunet.owl#</NAMESPACE>
      <LABEL>lom</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2001/vcard-rdf/3.0#</NAMESPACE>
      <LABEL>vcard</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://purl.org/dc/terms/</NAMESPACE>
      <LABEL>dcterms</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2000/01/rdf-schema#</NAMESPACE>
      <LABEL>rdfs</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2002/07/owl#</NAMESPACE>
      <LABEL>owl</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://dbpedia.org/ontology</NAMESPACE>
      <LABEL>dbpedia</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/1999/02/22-rdf-syntax-ns#</NAMESPACE>
      <LABEL>rdf</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://purl.org/dc/elements/1.1/</NAMESPACE>
      <LABEL>dc</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2004/02/skos/core#</NAMESPACE>
      <LABEL>skos</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.europeana.eu/schemas/edm/</NAMESPACE>
      <LABEL>europeana</LABEL>
   </PREFIX>
   <SOURCE>
      <ID>GLOBE</ID>
      <ENDPOINT>http://localhost:3030/globe/query</ENDPOINT>
      <VAR>?x</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>FILTER(langMatches(lang(?v0),"en"))</RESTRICTION>
      <PROPERTY>dcterms:title</PROPERTY>
   </SOURCE>
   <TARGET>
      <ID>DBpedia</ID>
      <ENDPOINT>http://dbpedia.org/sparql</ENDPOINT>
      <VAR>?y</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>FILTER(langMatches(lang(?v0),"en"))</RESTRICTION>
      <PROPERTY>rdfs:label</PROPERTY>
   </TARGET>
   <METRIC>trigrams(x.dcterms:title,y.rdfs:label)</METRIC>
   <ACCEPTANCE>
      <THRESHOLD>0.95</THRESHOLD>
      <FILE>../results/GLOBE2DBpedia_accepted.nt</FILE>
      <RELATION>owl:sameAs</RELATION>
   </ACCEPTANCE>
   <REVIEW>
      <THRESHOLD>0.75</THRESHOLD>
      <FILE>../results/GLOBE2DBpedia_reviewed.nt</FILE>
      <RELATION>owl:sameAs</RELATION>
   </REVIEW>
   <EXECUTION>nt</EXECUTION>
   <OUTPUT>nt</OUTPUT>
</LIMES>
